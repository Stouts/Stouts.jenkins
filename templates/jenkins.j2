# This file was generated by Ansible for {{ansible_fqdn}}
# Do NOT modify this file by hand!

NAME={{jenkins_name}}
JAVA={{jenkins_java}}
JAVA_ARGS="{{ jenkins_java_args|default([])|join(' ') }}"
PIDFILE={{jenkins_pidfile}}
JENKINS_USER={{jenkins_user}}
JENKINS_GROUP={{jenkins_group}}
JENKINS_WAR={{jenkins_war}}
JENKINS_HOME={{jenkins_home}}
RUN_STANDALONE={{'true' if jenkins_run_standalone else 'false'}}
JENKINS_LOG=/var/log/jenkins/$NAME.log
MAXOPENFILES=8192
HTTP_PORT={{jenkins_http_port}}
PREFIX={{jenkins_prefix}}

# arguments to pass to jenkins.
# --javahome=$JAVA_HOME
# --httpPort=$HTTP_PORT (default 8080; disable with -1)
# --httpsPort=$HTTP_PORT
# --ajp13Port=$AJP_PORT
# --argumentsRealm.passwd.$ADMIN_USER=[password]
# --argumentsRealm.roles.$ADMIN_USER=admin
# --webroot=~/.jenkins/war
# --prefix=$PREFIX
JENKINS_ARGS="--prefix=$PREFIX --webroot=/var/cache/jenkins/war --httpPort=$HTTP_PORT"
